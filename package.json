{
  "name": "cbidb-staff-web",
  "version": "0.1.0",
  "description": "CBI Staff Application.  Theme based on AppStack React by Paul Laros <support@bootlab.io>",
  "scripts": {
    "server": "webpack --env.platform=server",
    "web": "webpack --env.platform=web",
    "sass:build": "sass src/assets/scss:public/css --load-path node_modules",
    "sass:watch": "sass --watch src/assets/scss:public/css --load-path node_modules",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:build sass:autoprefixer",
    "build:js": "node scripts/build.js",
    "watch:css": "npm-run-all sass:watch",
    "watch:js": "node scripts/start.js",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*",
    "test": "mocha --require ts-node/register test/**/*.ts"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "not Edge < 79",
    "Firefox ESR",
    "iOS >= 10",
    "Safari >= 10",
    "Android >= 6",
    "not Explorer <= 11"

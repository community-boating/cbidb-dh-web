{
  "name": "cbidb-staff-web",
  "version": "0.1.0",
  "description": "CBI Staff Application.  Theme based on AppStack React by Paul Laros <support@bootlab.io>",
  "scripts": {
    "server": "webpack --env.platform=server",
    "web": "webpack --env.platform=web",
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "node scripts/build.js",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "node scripts/start.js",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@sentry/browser": "^5.19.1",
    "bootstrap": "^4.4.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chokidar-cli": "^2.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "css-loader": "^3.4.2",
    "file-loader": "^2.0.0",
    "fp-ts": "^1.18.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "io-ts": "^1.8.6",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-cli": "^7.1.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.0",
    "react-bootstrap-table2-toolkit": "^2.1.2",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.0",
    "react-redux-toastr": "^7.6.4",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.6",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "devDependencies": {
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.22",
    "@types/react-bootstrap-table-next": "^4.0.3",
    "@types/react-bootstrap-table2-paginator": "^2.1.0",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/reactstrap": "^8.4.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2"
  }
}
